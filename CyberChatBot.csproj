<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Media\welcome %28online-audio-converter.com%29.wav" />
    <None Remove="Media\welcome.wav" />
    <None Remove="Media\welcome.wav.mp3" />
    <None Remove="Media\welcome.wav.wav" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Media\welcome.wav">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Windows.Extensions" Version="9.0.4" />
  </ItemGroup>

</Project>
